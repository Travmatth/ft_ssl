        "stopAtEntry": false,
        "cwd": "${workspaceRoot}",
        "preLaunchTask": "build",
		"externalConsole": false,
		"setupCommands": [
			// {
				// used to pipe in.txt file through STDIN
				// "text": "settings set target.input-path in.txt"
			// }
		],
        "linux": {
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        "osx": {
            "MIMode": "lldb"
        },
        "windows": {
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    }
]
}