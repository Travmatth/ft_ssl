"version": "0.2.0",
"configurations": [
    {
        "name": "C++ Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceRoot}/ft_ssl",
        "args": ["des-ecb", "-i", "test/in_test", "-k", "0011223344556677", "-a"],
        "stopAtEntry": false,
        "cwd": "${workspaceRoot}",
        "preLaunchTask": "build",
		"externalConsole": false,
		"setupCommands": [
			{
				"text": "settings set target.input-path test/in_test"
			}
		],
        "linux": {
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        "osx": {
            "MIMode": "lldb"
        },
        "windows": {
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    }
]
}